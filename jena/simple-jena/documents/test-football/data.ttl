@prefix owl: <http://www.w3.org/2002/07/owl#> .

:Asia a :Area.

:VN a :Country;
  :inArea :Asia;
  :hasTeam :NT1.

:belongToArea rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasLeague ;
            rdfs:domain :League ;
            rdfs:range :Area .

:League1 a :League;
  :belongToArea :Asia.

:NT1 a :NationalTeam.

:LS1_1 a :LeagueSeason;
  :isLeague :League1;
  :heldInYear 2022;
  :hasChampion :NT1.

:P1 :hasPosition :Goalkeeper;
  :hasCountry :VN;
  :joinedYear 2010;
  :leftYear 2023.
:P2 :hasPosition :Forward;
  :name "p1";
  :hasCountry :VN;
  :joinedYear 2010;
  :leftYear 2013.
